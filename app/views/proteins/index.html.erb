<h1>Listing proteins</h1>

Enter word from protein description
<%= form_for(@peptide) do |f| %>
    <div class="field">
      <%= f.label :description %><br />
      <%= f.text_field :description %>
    </div>
<% end %>

<table>
  <tr>
    <th>Sp or tr</th>
    <th>Accession</th>
    <th>Description</th>
    <th>Aa sequence</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @proteins.each do |protein| %>
  <tr>
    <td><%= protein.sp_or_tr %></td>
    <td><%= protein.accession %></td>
    <td><%= protein.description %></td>
    <td><%= protein.aa_sequence %></td>
    <td><%= link_to 'Show', protein %></td>
    <td><%= link_to 'Edit', edit_protein_path(protein) %></td>
    <td><%= link_to 'Destroy', protein, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

